{
	"name": "wrap-your-code",
	"displayName": "wrap your code",
	"description": "Use to wrap your code with specified identity",
	"repository": {
    "type": "git",
    "url": "https://github.com/jean-ch/wrap-your-code.git"
  },
	"version": "1.0.0",
	"publisher": "jingxuan",
	"engines": {
		"vscode": "^1.41.0"
	},
	"categories": [
		"Formatters"
	],
	"activationEvents": [
		"onCommand:extension.format",
		"onCommand:extension.wrap"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.format",
				"title": "parse"
			},
			{
				"command": "extension.wrap",
				"title": "t"
			}
		],
		"keybindings": [
			{
				"command": "extension.wrap",
				"key": "ctrl+t",
				"mac": "cmd+t",
				"when": "editorHasSelection"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.41.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"typescript": "^3.6.4",
		"tslint": "^5.20.0",
		"vscode-test": "^1.2.2"
	},
	"dependencies": {}
}
